{
  "name": "filmdex",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/",
    "clean:build": "npm run clean && npm run build",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "dev:tmdb": "tsx src/crawlers/tmdb/tmdb.crawler.ts",
    "dev:tmdb:resume": "tsx src/scripts/tmdb-resume.ts",
    "start:tmdb": "npm run build && node dist/crawlers/tmdb/tmdb.crawler.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "db:studio": "npx prisma studio",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma migrate reset --force && npx prisma db push && npx prisma generate",
    "api:dev": "tsx src/api/server.ts",
    "api:build": "npm run build && node dist/api/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@crawlus/api": "^0.9.0",
    "@crawlus/core": "^0.9.0",
    "@crawlus/utils": "^0.9.0",
    "@prisma/client": "^6.13.0",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.2.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "got": "^14.4.7",
    "ow": "^2.0.0",
    "playwright": "^1.54.1",
    "prisma": "^6.13.0",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "user-agents": "^1.1.615",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "swagger-typescript-api": "^13.2.7",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
